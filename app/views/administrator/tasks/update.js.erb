<% if @task.is_completed? %>
  // $('#edit_task_<%= @task.id %>').appendTo('#complete_tasks')
  $('#incomplete_tasks #edit_task_<%= @task.id %>').remove()
  $('#complete_tasks').append('<%= j render(@task) %>')
<% else %>
  // $('#edit_task_<%= @task.id %>').appendTo('#incomplete_tasks')
  $('#complete_tasks #edit_task_<%= @task.id %>').remove()
  $('#incomplete_tasks').append('<%= j render(@task) %>')
<% end %>
