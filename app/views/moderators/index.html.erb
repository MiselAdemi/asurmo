<ul>
  <% @moderators_info.zip(@moderators).each do |user, moderator| %>
    <li><%= user.email %></li>
    <%= link_to("Delete Moderator", organization_moderator_path(@organization, moderator), :data => { :confirm => "Are you sure?" }, :method => :delete, :class => 'btn btn-danger') %>

    <%= link_to 'Add moderator', new_organization_moderator_path(@organization), :class => "btn btn-info" %>
  <% end %>
</ul>
